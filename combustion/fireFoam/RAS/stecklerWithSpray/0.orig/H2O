FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      H2O;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    out
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    outSide
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    sides
    {
        type            zeroGradient;
    }
    floor
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    burner
    {
        type            totalFlowRateAdvectiveDiffusive;
        massFluxFraction 0;
        phi             phi;
        rho             rho;
        value           uniform 0;
    }
}

