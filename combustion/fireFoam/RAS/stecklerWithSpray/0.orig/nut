FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    out
    {
        type            calculated;
        value           uniform 0;
    }
    outSide
    {
        type            calculated;
        value           uniform 0;
    }
    sides
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    floor
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    walls
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    burner
    {
        type            calculated;
        value           uniform 0;
    }
}

