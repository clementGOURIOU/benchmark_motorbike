simulationType LES;

RAS
{
    model               kOmegaSST;

    turbulence          on;

    printCoeffs         on;
}


LES
{
    LESModel        kEqn; 

    kEqn
    {
        ce               1.048;
        ck               0.094;
        Prt              0.7;
    }
    // kMin        1e-5;

    delta           cubeRootVol;

    cubeRootVolCoeffs
    {
        deltaCoeff      1;
    }

    turbulence      on;

    printCoeffs     on;
}

