            set baseFace;
        }
    }

    {
        name    extrusionAir;
        action  subset;
        type    faceSet;
        source  boxToFace;
        sourceInfo
        {
            box  (-0.001 -0.032 -0.022) (0.001 0.012 0.022);
        }
    }

