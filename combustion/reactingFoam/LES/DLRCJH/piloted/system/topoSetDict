actions
(
    {
        name    baseFace;
        action  new;
        type    faceSet;
        source  patchToFace;
        sourceInfo
        {
            name   "baseFront";
        }
    }

    {
        name    extrusionFuel;
        action  new;
        type    faceSet;
        source  faceToFace;
        sourceInfo
        {
            set baseFace;
        }
    }

    {
        name    extrusionFuel;
        action  subset;
        type    faceSet;
        source  boxToFace;
        sourceInfo
        {
            box  (-0.001 0.018 -0.034) (0.001 0.046 0.034);
        }
    }

    {
        name    extrusionAir;
        action  new;
        type    faceSet;
        source  faceToFace;
        sourceInfo
