FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type     calculated;
        value    $internalField;
    }

    // pilot, closed or open
    fuelInlet
    {
        type     zeroGradient;
    }

    outlet
    {
        type     calculated;
        value    $internalField;
    }

    DLRCJH
    {
        type     zeroGradient;
    }
}

// ************************************************************************* //
