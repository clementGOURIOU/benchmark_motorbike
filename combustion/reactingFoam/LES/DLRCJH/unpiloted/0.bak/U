FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "${FOAM_CASE}/constant/caseSettings"

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            flowRateInletVelocity;
        massFlowRate    constant    0.5563;
        extrapolateProfile          0;
        value           uniform ($../../inlet/U 0 0);
    }

    // pilot, closed or open 
    fuelInlet
    {
        type            noSlip;
    }

    outlet
    {
        type            zeroGradient;
    }

    DLRCJH
    {
        type            noSlip;
    }
}

