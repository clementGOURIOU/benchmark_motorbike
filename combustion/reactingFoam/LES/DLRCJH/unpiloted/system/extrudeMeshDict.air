FoamFile
{
    format      ascii;
    version     2.0;
    class       dictionary;
    object      extrudeMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

constructFrom mesh;

sourceCase ".";

sourcePatches (inlet);

flipNormals false;

extrudeModel        linearNormal;

nLayers             200;

expansionRatio      1.0;

linearNormalCoeffs
{
    thickness       0.39;
}

mergeFaces false;

mergeTol 0;

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
