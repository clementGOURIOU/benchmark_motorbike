FoamFile
{
    format      ascii;
    class       dictionary;
    object      extrudeMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

constructFrom mesh;

sourceCase ".";

sourcePatches (inlet);

flipNormals false;

extrudeModel        linearNormal;

nLayers             180;

expansionRatio      1.0;

linearNormalCoeffs
{
    thickness       0.39;
}

mergeFaces false;

mergeTol 0;

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
