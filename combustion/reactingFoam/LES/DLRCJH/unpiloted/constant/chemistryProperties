FoamFile
{
    format          ascii;
    class           dictionary;
    version         2;
    object          chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    solver          ode;
}

chemistry       on;

initialChemicalTimeStep 1e-08;

odeCoeffs
{
    solver          RKCK45;
    absTol          1e-12;
    relTol          0.1;
}

#include        "reactionSimple"


