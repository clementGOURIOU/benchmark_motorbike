FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1490;

boundaryField
{
    fuel_inlet
    {
        type            fixedValue;
        value           uniform 600;
    }
    air_inlet
    {
        type            fixedValue;
        value           uniform 1490;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    walls
    {
        type            zeroGradient;
    }
    sides
    {
        type            symmetry;
    }

}


