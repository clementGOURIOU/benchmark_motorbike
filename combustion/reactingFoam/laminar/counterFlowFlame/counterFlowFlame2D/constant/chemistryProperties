FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    solver            ode;
}

chemistry           on;

initialChemicalTimeStep 1e-07;

odeCoeffs
{
    solver          RKCK45;
    absTol          1e-12;
    relTol          0.01;
}

#include "reactions"

