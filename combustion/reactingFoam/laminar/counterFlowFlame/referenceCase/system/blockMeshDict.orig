FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

nCellsX         100;
nCellsY         #calc "int($nCellsX / 2.5)";

scale           1;

vertices        
(
    ( 0 -0.01 -0.001 )
    ( 0.02 -0.01 -0.001 )
    ( 0.02 0.01 -0.001 )
    ( 0 0.01 -0.001 )
    ( 0 -0.01 0.001 )
    ( 0.02 -0.01 0.001 )
    ( 0.02 0.01 0.001 )
    ( 0 0.01 0.001 )
);

blocks          
( 
    hex ( 0 1 2 3 4 5 6 7 ) ( $nCellsX $nCellsY 1 ) simpleGrading ( 1 1 1 ) 
);

edges
();

boundary
( 
    fuel 
    { 
        type patch;
        faces
        (
            ( 0 4 7 3 ) 
        );
    } 
    air 
    { 
        type patch;
        faces 
        ( 
            ( 1 2 6 5 )
        ); 
    } 
    outlet 
    { 
        type patch;
        faces 
        ( 
            ( 0 1 5 4 ) 
            ( 7 6 2 3 ) 
        );
    } 
    frontAndBack 
    { 
        type empty ; 
        faces 
        ( 
            ( 4 5 6 7 ) 
            ( 0 3 2 1 ) 
        );
    }
);

mergePatchPairs 
();

// ************************************************************************* //
