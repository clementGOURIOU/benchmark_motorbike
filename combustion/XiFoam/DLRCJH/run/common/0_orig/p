FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "${FOAM_CASE}/includeSetup"

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform $pOutlet;

boundaryField
{
    outlet
    {
        type            waveTransmissive;
        gamma           1.3;
        fieldInf        $pOutlet;
        lInf            5;
        value           uniform $pOutlet;
    }

    "(wall.*|inletsPilots|inletMain)"
    {
        type            zeroGradient;
    }
}

