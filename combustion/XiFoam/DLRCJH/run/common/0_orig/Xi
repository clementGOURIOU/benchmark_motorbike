FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Xi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{

    "(inletMain|inletsPilots)"
    {
        type            fixedValue;
        value           uniform 1;
    }

    "(wall.*)"
    {
        type            zeroGradient;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 20;
        value           uniform 20;
    }
}

