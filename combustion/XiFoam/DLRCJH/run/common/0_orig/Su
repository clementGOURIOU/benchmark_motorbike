FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Su;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "${FOAM_CASE}/includeSetup"

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform $SuGuelder;

boundaryField
{

    "(inletMain|outlet|inletsPilots)"
    {
        type            calculated;
        value           uniform $SuGuelder;
    }

    "(wall.*)"
    {
        type            zeroGradient;
    }
}

