FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(p.*|rho.*)"
    {
        solver          PCG;
        preconditioner  DIC;
        minIter         6;
        maxIter         6;
    }

    "U.*"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        minIter         2;
        maxIter         2;
    }

    "(b.*|Xi.*|hau.*|k.*)"
    {
        solver          smoothSolver;
        smoother        DILUGaussSeidel;
        minIter         2;
        maxIter         2;
    }

    // regexp "ha.*" would include "hau|hauFinal"
    "(ha|haFinal|ft.*)"
    {
        solver          smoothSolver;
        smoother        DILUGaussSeidel;
        minIter         1;
        maxIter         1;
    }
}

PIMPLE
{
    nOuterCorrectors 2;
    nCorrectors     1;
    nNonOrthogonalCorrectors 0;
}

