FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#includeEtc "caseDicts/postProcessing/graphs/sampleDict.cfg"

type		sets;

libs		(sampling);

writeControl    writeTime;

interpolationScheme cellPoint;

setFormat     raw;

sets
(
    plane0_Yaxis //Resulting file name
    {
        type            uniform;
        axis            y;         //x-axis in plot
        start           (0 0 0);
        end             (0 0.255 0);
        nPoints         500;
    }
    plane1_Yaxis //Resulting file name
    {
        type            uniform;
        axis            y;         //x-axis in plot
        start           (0.025 -0.255 0);
        end             (0.025 0.255 0);
        nPoints         500;
    }
    plane2_Yaxis //Resulting file name
    {
        type            uniform;
        axis            y;         //x-axis in plot
        start           (0.05 -0.255 0);
        end             (0.05 0.255 0);
        nPoints         500;
    }
    plane4_Yaxis //Resulting file name
    {
        type            uniform;
        axis            y;         //x-axis in plot
        start           (0.1 -0.255 0);
        end             (0.1 0.255 0);
        nPoints         500;
    }
    plane5_Yaxis //Resulting file name
    {
        type            uniform;
        axis            y;         //x-axis in plot
        start           (0.125 -0.255 0);
        end             (0.125 0.255 0);
        nPoints         500;
    }
    plane7_Yaxis //Resulting file name
    {
        type            uniform;
        axis            y;         //x-axis in plot
        start           (0.175 -0.255 0);
        end             (0.175 0.255 0);
        nPoints         500;
    }
);

surfaces
();

fields
(
    UMean
    UPrime2Mean
);

