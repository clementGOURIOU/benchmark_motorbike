FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "p.*"
    {
        solver          PCG;
        preconditioner  DIC;
        minIter         14;
        maxIter         14;
    }

    "(rho.*|U.*|b.*|Xi.*|k.*|ha|haFinal)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        minIter         1;
        maxIter         1;
    }

    "(hau.*)"
    {
        $U;
        minIter         2;
        maxIter         2;
    }
}

PIMPLE
{
    nOuterCorrectors 2;
    nCorrectors     1;
    nNonOrthogonalCorrectors 0;
}

