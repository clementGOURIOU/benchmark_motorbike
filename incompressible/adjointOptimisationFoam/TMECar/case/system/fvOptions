FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

velocityDamping
{
	// Mandatory entries
	type            velocityDampingConstraint;
	UMax            120;

	// Optional entries
	selectionMode   all;
	UNames          (U); // names of given velocity fields

	// Optional fvOptions entries
	active          true;
}

limitU
{
	type            limitVelocity;
	active          yes;

	selectionMode   all;
	UNames          (U); // names of given velocity fields
	
    max             160;
}


