FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(p|pa|Phi).*"
    {
        solver                      GAMG;
        tolerance                   1.e-6;
        relTol                      0.1;
        smoother                    symGaussSeidel;
        nPreSweeps                  0;
        nPostSweeps                 2;
        postSweepsLevelMultiplier   1;
        maxPostSweeps               4;
        nFinestSweeps               2;
        interpolateCorrection       no;
        cacheAgglomeration          true;
        nCellsInCoarsestLevel       10;
        agglomerator                algebraicPair;
        mergeLevels                 1;
    //    minIter                     3;
    }
    ma
    {
        solver                      GAMG;
        tolerance                   1.e-9;
        relTol                      0.1;
        smoother                    symGaussSeidel;
        nPreSweeps                  0;
        nPostSweeps                 2;
        postSweepsLevelMultiplier   1;
        maxPostSweeps               4;
        nFinestSweeps               2;
        interpolateCorrection       no;
        cacheAgglomeration          true;
        nCellsInCoarsestLevel       10;
        agglomerator                algebraicPair;
        mergeLevels                 1;
    //    minIter                     3;
    }
    "(p|pa).*Final"
    {
        $p;
        relTol                      0;
    }
    "(U|Ua|nuTilda).*"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-7;
        relTol          0.1;
    }
    "(U|Ua|nuTilda).*Final"
    {
        $U;
        relTol          0;
    }
    "yWall|da"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-9;
        relTol          0.1;
    }
}

relaxationFactors
{
    fields
    {
        "p.*"               1.;
        "p.*Final"          1.;
        "pa.*"              1.;
        "pa.*Final"         1.;
    }
    equations
    {
        "U.*"               1.;
        "U.*Final"          1.;
        "nuTilda.*"         1.;
        "nuTilda.*Final"    1.;
        "Ua.*"              1.;
        "Ua.*Final"         1.;
        "yWall.*"           0.7;
        "da.*"              0.5;
    }
}

