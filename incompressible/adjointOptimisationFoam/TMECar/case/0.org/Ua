	}

	wheels_tyres_front
	{
		type        adjointWallVelocity;
		value	    uniform (0. 0. 0.);
	}

	wheels_tyres_front_no_layers
	{
		type        adjointWallVelocity;
		value	    uniform (0. 0. 0.);
	}

	wheels_tyres_rear
	{
		type        adjointWallVelocity;
		value	    uniform (0. 0. 0.);
	}

	wheels_tyres_rear_no_layers
	{
