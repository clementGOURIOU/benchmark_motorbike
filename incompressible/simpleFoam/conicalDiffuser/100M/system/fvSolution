solvers
{
    p
    {
        solver          PCG;
        preconditioner
        {
            preconditioner  GAMG;
            coarsestLevelCorr
            {
                solver          PCG;
                preconditioner  DIC;
                minIter         7;
                maxIter         7;
            }
            smoother        DICGaussSeidel;
            cacheAgglomeration no;
        }
        minIter         2;
        maxIter         2;
    };

    Phi
    {
        $p
    };

    U
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        minIter         3;
        maxIter         3;
    };

    "k|omega"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        minIter         4;
        maxIter         4;
    };
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
    pRefCell                 0;
    pRefValue                0;
}

