// Geometric parameters
lref       2.78618;                        // Reference length/wheelbase (in m)
Aref       2.17;                           // Reference area (in m^2) - frontal area

// Patch assignment:
rotatingWallPatches         "(Rimsfront|BrakeDiscfront|Tiresfront)";
rotatingWallPatchesRear     "(Rimsrear|BrakeDiscrear|Tiresrear)";
movingMeshWallPatches       "(Rimsfront_rota|Rimsrear_rota|BrakeDiscfront_rota|BrakeDiscrear_rota)";
movingGroundWallPatches     "(ground|ground_layer)";
inletPatch                  inlet;
outletPatch                 outlet;
fixedWallPatches            "(Body.*|Closed.*|CTRL.*|Engine.*|Exhaust.*|Mirror.*|Notchback.*|OCDA.*|Powertrain|Wheel.*)";
symmPatches                 "(freestream)";
forcesWallPatches           "(Body.*|Closed.*|CTRL.*|Engine.*|Exhaust.*|Mirror.*|Notchback.*|OCDA.*|Powertrain|Wheel.*|Rimsfront|BrakeDiscfront|Tiresfront|Rimsrear|BrakeDiscrear|Tiresrear|Rimsfront_rota|Rimsrear_rota|BrakeDiscfront_rota|BrakeDiscrear_rota)"; 

// Inlet and boundary conditions:
Uinf                (38.889 0 0);    // Reference velocity vector at inlet (in m/s)
UinfMag             38.889;          // Reference velocity magnitude at inlet (in m/s)
nu                  1.507e-05;       // Reference kinematic viscosity (in m^2/s) - air at 20Â°C
pref                0.0;             // Reference pressure at outlet (in m^2/s^2)
viscRatio           5.0;             // Turbulent viscosity ratio in domain
Tu                  0.0026;          // Turbulence intensity in domain

// Turbulence model:
LESturbModel        kOmegaSSTDDES;   // Model for LES if used
RASturbModel        kOmegaSST;       // Model for RANS simulations if used
deltaTurb           maxDeltaxyz;     // Selected delta formulation

// Simulation parameters:
dt                  1e-4;            // Timestep
tAvg                0.2;             // Time when to start averaging (in s) - 0.05s approx. equal to 1 full rotation of the wheel
tEnd                0.5;             // End time of simulation (in s)
writeInterval_      100;             // Restart write interval
nCores              128;              // Number of cores used for simulation

// Probe settings:
interpScheme cellPatchConstrained;
