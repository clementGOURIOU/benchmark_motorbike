FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dynamicFvMesh   dynamicMultiMotionSolverFvMesh;

motionSolverLibs (fvMotionSolvers);

dynamicMultiMotionSolverFvMeshCoeffs
{
    ACMI_FL
    {
        solver    solidBody;

        cellZone        rotatingCells_FL;
        
        solidBodyCoeffs
        {
             solidBodyMotionFunction  rotatingMotion;
             rotatingMotionCoeffs
             {
                  origin      (0.007 0 0);
                  axis        (0 1 0);
                  omega       -122.899552; // rad/s
             }
        }
    }
    ACMI_FR
    {
        solver    solidBody;
    
        cellZone        rotatingCells_FR;
        solidBodyCoeffs
        {
             solidBodyMotionFunction  rotatingMotion;
             rotatingMotionCoeffs
             {
                  origin      (0.007 0 0);
                  axis        (0 1 0);
                  omega       -122.899552; // rad/s
             }
        }

    }
    ACMI_RL
    {
        solver    solidBody;
    
        cellZone        rotatingCells_RL;
   
        solidBodyCoeffs
        {
             solidBodyMotionFunction  rotatingMotion;
             rotatingMotionCoeffs
             {
                  origin      (2.79318452 0 0);
                  axis        (0 1 0);
                  omega       -122.899552; // rad/s
             }
        }
 
    }
    ACMI_RR
    {
        solver    solidBody;
    
        cellZone        rotatingCells_RR;
   
        solidBodyCoeffs
        {
             solidBodyMotionFunction  rotatingMotion;
             rotatingMotionCoeffs
             {
                  origin      (2.79318452 0 0);
                  axis        (0 1 0);
                  omega       -122.899552; // rad/s
             }
        }
 
    }

}
