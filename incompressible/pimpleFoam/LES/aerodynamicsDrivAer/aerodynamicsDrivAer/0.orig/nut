FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include            "$FOAM_CASE/constant/turbulenceQuantities"

dimensions          [ 0 2 -1 0 0 0 0 ];
internalField       uniform $turbulentNut;

boundaryField
{
    groupWall
    {
        type        nutUSpaldingWallFunction;
        value       uniform $turbulentNut;
    }

    groupSymmetry
    {
        type        symmetry;
    }

    groupDefault
    {
        type        nutUSpaldingWallFunction;
        value       uniform $turbulentNut;
    }

    groupTransition
    {
        type        nutLowReWallFunction;
        value       uniform 0;
    }

    groupSlip
    {
        type        slip;
    }

    groupGRWS_Tire_FL
    {
        type        nutUSpaldingWallFunction;
        value       uniform $turbulentNut;
    }

    groupGRWS_Tire_FR
    {
        type        nutUSpaldingWallFunction;
        value       uniform $turbulentNut;
    }

    groupGRWS_Tire_RL
    {
        type        nutUSpaldingWallFunction;
        value       uniform $turbulentNut;
    }

    groupGRWS_Tire_RR
    {
        type        nutUSpaldingWallFunction;
        value       uniform $turbulentNut;
    }

    groupWtunInlet
    {
        type        fixedValue;
        value       uniform $turbulentNut;
    }

    groupWtunOutlet
    {
        type        inletOutlet; 
        inletValue  uniform $turbulentNut;
        value       uniform $turbulentNut;
    }

    groupWtunStreet
    {
        type        nutUSpaldingWallFunction;
        value       uniform $turbulentNut;
    }

    // Set patchGroups for constraint types
    #include "${WM_PROJECT_DIR}/etc/caseDicts/setConstraintTypes"
}

// ************************************************************************* //
