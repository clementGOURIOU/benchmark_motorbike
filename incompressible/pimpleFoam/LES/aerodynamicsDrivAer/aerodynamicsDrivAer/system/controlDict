FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;
startFrom       startTime;
startTime       0;
stopAt          endTime;
endTime         4000;
deltaT          1;
writeControl    timeStep;
writeInterval   1000;
purgeWrite      1;
writeFormat     binary;
writePrecision  8;
writeCompression no;
timeFormat      general;
timePrecision   8;
runTimeModifiable yes;

functions
{
    yPlus
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             yPlus;
        writeControl     writeTime;
        writeInterval    1000;
        executeControl   timeStep;
        executeInterval  1000;
        timeStart        2000;
        log              no;
        result           yPlus;
    }
    
    wallShearStress
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             wallShearStress;
        writeControl     writeTime;
        writeInterval    1000;
        executeControl   timeStep;
        executeInterval  1000;
        timeStart        2000;
        log              false;
        patches
        (
            DrivAer_PID__00
            DrivAer_PID__01
            DrivAer_PID__02
            DrivAer_PID__03
            DrivAer_PID__04
