            DrivAer_PID__34
            DrivAer_PID__35
            Tire_FL
            Tire_FR
            Tire_RL
            Tire_RR
        );
        result           wallShearStress;
    }
    
    fieldAverage
    {
        libs              ( "libfieldFunctionObjects.so" );
        type              fieldAverage;
        writeControl      writeTime;
        writeInterval     1000;
        executeControl    timeStep;
        executeInterval   1000;
        timeStart         2000;
        restartOnRestart  false;
        restartOnOutput   false;
        periodicRestart   false;
        avgControls
        {
            mean        on;
            prime2Mean  off;
            base        time;
        }
        fields
        (
            U { $avgControls; }
            p { $avgControls; }
            CourantNo { $avgControls; }
            phi { $avgControls; }
            wallShearStress { $avgControls; }
            yPlus { $avgControls; }
        );
    }
    
    MinMax
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             fieldMinMax;
        writeControl     timeStep;
        writeInterval    1;
        executeControl   timeStep;
        executeInterval  1;
        timeStart        0;
        fields
        (
            p
            U
        );
    }

    forceCoeffs
    {
        libs             ( "libforces.so" );
        type             forceCoeffs;
        writeControl     timeStep;
        writeInterval    1;
        executeControl   timeStep;
        executeInterval  1;
        timeStart        0;
        p                p;
        U                U;
        rho              rhoInf;
        log              true;
        rhoInf           1.1584;
        liftDir          (0 0 1);
        dragDir          (1 0 0);
        CofR             (1.4000899999999998 0.0 -0.31751999999999997);
        pitchAxis        (0 1 0);
        magUInf          38.888888888888886;
        lRef             2.78618;
        Aref             2.2;
        porosity         false;
        patches 
        (
            DrivAer_PID__00
            DrivAer_PID__01
            DrivAer_PID__02
            DrivAer_PID__03
            DrivAer_PID__04
            DrivAer_PID__05
