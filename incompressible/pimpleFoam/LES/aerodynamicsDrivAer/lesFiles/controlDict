FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         pimpleFoam;
startFrom           latestTime;
startTime           0;
stopAt              endTime;
endTime             2.0;
deltaT              0.0001;
writeControl        runTime;
writeInterval       1;
purgeWrite          1;
writeFormat         binary;
writePrecision      10;
writeCompression    off;
timeFormat          general;
timePrecision       10;
runTimeModifiable   true;
adjustTimeStep      false;

functions
{
    CourantNo
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             CourantNo;
        writeControl     writeTime;
        writeInterval    1;
        executeControl   timeStep;
        executeInterval  1;
        timeStart        1.0;
        result           CourantNo;
    }
    
    yPlus
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             yPlus;
        writeControl     writeTime;
        writeInterval    1;
        executeControl   timeStep;
        executeInterval  1;
        timeStart        1.0;
        log              no;
        result           yPlus;
    }
    
    wallShearStress
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             wallShearStress;
        writeControl     writeTime;
        writeInterval    1;
        executeControl   timeStep;
        executeInterval  1;
        timeStart        1.0;
        log              false;
        patches
        (
            DrivAer_PID__00
            DrivAer_PID__01
            DrivAer_PID__02
            DrivAer_PID__03
            DrivAer_PID__04
            DrivAer_PID__05
