            DrivAer_PID__33
            DrivAer_PID__34
            DrivAer_PID__35
            Tire_FL
            Tire_FR
            Tire_RL
            Tire_RR
        );
        result           wallShearStress;
    }
    
    fieldAverage
    {
        libs              ( "libfieldFunctionObjects.so" );
        type              fieldAverage;
        writeControl      writeTime;
        writeInterval     1;
        executeControl    timeStep;
        executeInterval   1;
        timeStart         2.0;
        restartOnRestart  false;
        restartOnOutput   false;
        periodicRestart   false;
        avgControls
        {
            mean        on;
            prime2Mean  off;
            base        time;
        }
        fields
        (
            U { $avgControls; }
            p { $avgControls; }
            CourantNo { $avgControls; }
            phi { $avgControls; }
            wallShearStress { $avgControls; }
            yPlus { $avgControls; }
        );
    }
    
    StaticPressurePascalMean
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             pressure;
        writeControl     onEnd;
        writeInterval    1;
        executeControl   onEnd;
        executeInterval  1;
        timeStart        1.0;
        mode             static;
        rhoInf           1.1584;
        rho              rhoInf;
        pRef             0;
        pInf             0;
        field            pMean;
        U                UMean;
        UInf             ( 38.888888888888886 0 0 );
        result           pMeanStatic;
    }
    
    StaticPressureCoeffMean
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             pressure;
        writeControl     onEnd;
        writeInterval    1;
        executeControl   onEnd;
        executeInterval  1;
        timeStart        1.0;
        mode             staticCoeff;
        rhoInf           1.1584;
        rho              rhoInf;
        pRef             0;
        pInf             0;
        field            pMean;
        U                UMean;
        UInf             ( 38.888888888888886 0 0 );
        result           pMeanCoeffStatic;
    }
    
    TotalPressurePascalMean
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             pressure;
        writeControl     onEnd;
        writeInterval    1;
        executeControl   onEnd;
        executeInterval  1;
        timeStart        1.0;
        mode             total;
        rhoInf           1.1584;
        rho              rhoInf;
        pRef             0;
        pInf             0;
        field            pMean;
        U                UMean;
        UInf             ( 38.888888888888886 0 0 );
        result           pMeanTotal;
    }
    
    TotalPressureCoeffMean
    {
        libs             ( "libfieldFunctionObjects.so" );
        type             pressure;
        writeControl     onEnd;
        writeInterval    1;
