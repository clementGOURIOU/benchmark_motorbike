FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 1);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 11.6);
    }
    rotSwirlWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    statSwirlWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wallDiffuser
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wallOutletPipe
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           $internalField;
    }
}

// ************************************************************************* //
