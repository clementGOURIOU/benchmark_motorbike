FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(rho)"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-12;
        relTol          0.01;
    }

    "(rho)Final"
    {
        $rho;
        relTol          0;
    }

    "(p|U|e|k|nuTilda)"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-12;
        relTol          0.01;

     /* solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-12;
        relTol          0.01;
     */
    }

    "(p|U|e|k|nuTilda)Final"
    {
        $U;
        relTol          0;
    }
}

PIMPLE
{
    momentumPredictor yes;
    transonic         no;
    nOuterCorrectors 1;
    nCorrectors     2;
    nNonOrthogonalCorrectors 0;
    rhoMin          rhoMin [ 1 -3 0 0 0 ] 0.05;
    rhoMax          rhoMax [ 1 -3 0 0 0 ] 20.0;
}

relaxationFactors
{
    fields
    {
    }
    equations
    {
    }
}

