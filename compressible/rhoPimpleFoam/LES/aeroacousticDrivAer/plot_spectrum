set title "Spectra on side window \n{Samples: 1000 in a window; Freq resolution: 50Hz}"
set ylabel 'PSD, dB/Hz'
set xlabel 'Frequency, Hz'
set yrange [50:130]
set xrange [50:10000]
set grid
set grid mxtics
set terminal png size 1000,750
set output "PSD_spectrum-plot-p1-p5.png"

set logscale x
plot "<cat postProcessing/noise/probes-1/pointNoise/input0/p/PSD_dB_Hz_f.dat" using 1:2 title "p1" with lines, \
     "<cat postProcessing/noise/probes-2/pointNoise/input0/p/PSD_dB_Hz_f.dat" using 1:2 title "p2" with lines, \
     "<cat postProcessing/noise/probes-3/pointNoise/input0/p/PSD_dB_Hz_f.dat" using 1:2 title "p3" with lines, \
     "<cat postProcessing/noise/probes-4/pointNoise/input0/p/PSD_dB_Hz_f.dat" using 1:2 title "p4" with lines, \
     "<cat postProcessing/noise/probes-5/pointNoise/input0/p/PSD_dB_Hz_f.dat" using 1:2 title "p5" with lines

unset logscale x
set title "Pressure history"
set ylabel 'Pressure, Pa'
set xlabel 'Time, s'
set yrange [98500:102500]
unset xrange
set output "Pressure_history-p1-5-patchProbes.png"
plot "<cat postProcessing/patchProbes/0/p" using 1:2 title "p1" with lines, \
     "<cat postProcessing/patchProbes/0/p" using 1:3 title "p2" with lines, \
     "<cat postProcessing/patchProbes/0/p" using 1:4 title "p3" with lines, \
     "<cat postProcessing/patchProbes/0/p" using 1:5 title "p4" with lines, \
     "<cat postProcessing/patchProbes/0/p" using 1:6 title "p5" with lines
