FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-05;

boundaryField
{
    
    ".*"
    {
        type        zeroGradient;
    }

    #includeEtc "caseDicts/setConstraintTypes"
       
    XMin
    {
        type            fixedValue;
        value           uniform 0.001;
    }
    
    XMax
    {
        type            inletOutlet;
        inletValue      uniform 0.001;
        value           uniform 0.001;
    }
    
    YMin
    {
        type            symmetry;
    }
    
    YMax
    {
        type            symmetry;
    }
    
    ZMax
    {
        type            symmetry;
    }
}
