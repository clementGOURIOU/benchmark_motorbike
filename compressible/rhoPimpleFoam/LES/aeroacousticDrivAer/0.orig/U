FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (38.889 0 0);

boundaryField
{
    
    ".*"
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    #includeEtc "caseDicts/setConstraintTypes"

    XMin
    {
        type            surfaceNormalFixedValue;
        refValue        uniform -38.889;
    }
    
    XMax
    {
        type            inletOutlet;
        value           uniform (0 0 0);
        inletValue      uniform (0 0 0);
    }
    
    YMin
    {
        type            symmetry;
    }
    
    YMax
    {
        type            symmetry;
    }
    
    ZMax
    {
        type            symmetry;
    }
    
    ZMin
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}
