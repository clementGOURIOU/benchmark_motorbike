set title "Spectra: \n{Time window: 1.5-2.5s; Freq resolution: 1Hz}"
set ylabel 'SPL, dB'
set xlabel 'Frequency, Hz'
set grid
set grid mxtics
set yrange [0:150]
set terminal png size 1000,750
set output "SPL_spectrum-w-1.5-2.5_plot-p1.png"

set logscale x
plot "<cat postProcessing/noise/pressurePoint1/pointNoise/input0/pGauge/SPL_dB_f.xy" using 1:2 title "p1" with lines

set output "SPL_spectrum-w-1.5-2.5_plot-p2.png"
plot "<cat postProcessing/noise/pressurePoint2/pointNoise/input0/pGauge/SPL_dB_f.xy" using 1:2 title "p2" with lines

set output "SPL_spectrum-w-1.5-2.5_plot-p3.png"
plot "<cat postProcessing/noise/pressurePoint3/pointNoise/input0/pGauge/SPL_dB_f.xy" using 1:2 title "p3" with lines

set output "SPL_spectrum-w-1.5-2.5_plot-p4.png"
plot "<cat postProcessing/noise/pressurePoint4/pointNoise/input0/pGauge/SPL_dB_f.xy" using 1:2 title "p4" with lines

set output "SPL_spectrum-w-1.5-2.5_plot-p5.png"
plot "<cat postProcessing/noise/pressurePoint5/pointNoise/input0/pGauge/SPL_dB_f.xy" using 1:2 title "p5" with lines

unset logscale x
set title "Pressure history"
set ylabel 'Pressure, Pa'
set xlabel 'Time, s'
set grid
set yrange [-1500:1500]
set grid mxtics
set terminal png size 1000,750
set output "Pressure_history-p1-5.png"
plot "<cat postProcessing/probes_pGauge/0/pGauge" using 1:2 title "p1" with lines, \
     "<cat postProcessing/probes_pGauge/0/pGauge" using 1:3 title "p2" with lines, \
     "<cat postProcessing/probes_pGauge/0/pGauge" using 1:4 title "p3" with lines, \
     "<cat postProcessing/probes_pGauge/0/pGauge" using 1:5 title "p4" with lines, \
     "<cat postProcessing/probes_pGauge/0/pGauge" using 1:6 title "p5" with lines

set title "Force(Y) history"
set ylabel 'Force, N'
set xlabel 'Time, s'
set grid
set yrange [300:400]
set grid mxtics
set terminal png size 1000,750
set output "Force_history.png"
plot "<cat postProcessing/forces_all/0/force.dat" using 1:3 title "Fy" with lines

set output "Moment_history.png"
set title "Moment(Y) history"
set ylabel 'Moment, N-m'
set xlabel 'Time, s'
set yrange [-30:-10]
plot "<cat postProcessing/forces_all/0/moment.dat" using 1:3 title "My" with lines
