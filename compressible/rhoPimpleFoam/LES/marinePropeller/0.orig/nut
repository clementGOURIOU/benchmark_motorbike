FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-05;

boundaryField
{
    //- Set patchGroups for constraint patches
    #includeEtc "caseDicts/setConstraintTypes"    
    
    freestream
    {
        type            calculated;
        value           $internalField;
    }

    "propeller.*"
    {
        type            nutUSpaldingWallFunction;
        value           $internalField;
    }
    
    propellerStem_outlet
    {
        type            calculated;
        value           $internalField;
    }

    "AMI.*"
    {
        type            cyclicAMI;
        value           $internalField;
    }

}

