FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{

    "(rho|rhoFinal)"
    {
        solver          diagonal;
    }

    "(p|Phi)"
    {
        solver          GAMG;
        tolerance       1e-20;
        relTol          0.05;
        smoother        symGaussSeidel;
        cacheAgglomeration true;
        nCellsInCoarsestLevel 200;
        agglomerator    faceAreaPair;
        nPreSweeps      0;
        nPostSweeps     2;
        mergeLevels     1;
    }

    a
    {
        solver          PCG;
        preconditioner  FDIC;
        tolerance       1e-20;
        relTol          0.01;
        maxIter         600;
    }

    "(U|k|omega|nuTilda)"
    {
//        solver          smoothSolver;
//        smoother        symGaussSeidel;
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-20;
        relTol          0.1;
    }

    "(h|e|hFinal|eFinal)"
    {
        $U;
        tolerance       1.0;
        relTol          1.0;
    }
}

potentialFlow
{
    nNonOrthogonalCorrectors 10;
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
    transonic           no;
    pMax 		1.0e+08;
    pMin 		-1.0e+08;
}

relaxationFactors
{
        U               0.7;
        p               0.3;
        h               0.9;
        e               0.9;
        k               0.5;
        nuTilda         0.5;
        omega           0.5;
}

