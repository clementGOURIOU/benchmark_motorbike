FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pOut            101325;

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform $pOut;

boundaryField
{
    //- Set patchGroups for constraint patches
    #includeEtc "caseDicts/setConstraintTypes"

    atmosphere
    {
        type            freestreamPressure;
        freestreamValue uniform $pOut;
    }

    cylinder
    {
        type            zeroGradient;
    }

    symm
    {
        type            slip;
        value           $internalField;
    }

}

