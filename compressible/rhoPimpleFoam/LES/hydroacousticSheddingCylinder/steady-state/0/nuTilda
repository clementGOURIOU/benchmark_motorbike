FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.0e-03;

boundaryField
{
    //- Set patchGroups for constraint patches
    #includeEtc "caseDicts/setConstraintTypes"

    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 9.1856e-03;
        value           uniform 9.1856e-03;

    }
    
    cylinder
    {
        type            zeroGradient;
    }

    symm
    {
        type            slip;
        value           $internalField;
    }
}

