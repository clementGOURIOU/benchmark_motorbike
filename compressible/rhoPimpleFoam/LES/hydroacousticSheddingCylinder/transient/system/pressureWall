    pressureWall_cylinder
    {
        type                    surfaces;
        libs                    ("libsampling.so");
        timeStart               0.1;
        writeControl            timeStep;
        writeInterval           2;
        interpolationScheme     cellPoint;
        surfaceFormat           ensight;

        fields ( p );

        surfaces
        (
                FSP
                {
                        type    patch;
                        patches ( dome cylinder );
                        triangulate     false;
                }
        );
    }

    pressureWall_symm
    {
        type                    surfaces;
        libs                    ("libsampling.so");
        timeStart               0.01;
        writeControl            timeStep;
        writeInterval           2;
        interpolationScheme     cellPoint;
        surfaceFormat           ensight;

        fields ( p );

        surfaces
        (
                FSP
                {
                        type    patch;
                        patches ( symm );
                        triangulate     false;
                }
        );
    }

