    force_coefficient
    {
        type        forceCoeffs;
        libs        ( "libforces.so" );
        writeControl timeStep;
        writeInterval 1;
        patches     ( cylinder);
        p           p;
        U           U;
        rho         rho;             // compressible
        log         true;
        rhoInf      997;              // Redundant for incompressible
        liftDir     (0 1 0);
        dragDir     (1 0 0);
        CofR        (0 0 0); // Axle midpoint on ground
        pitchAxis   (0 0 1);
        magUInf     15;
        lRef        0.003;        // Wheelbase length
        Aref        6.44413e-04;         // Estimated
        porosity    false;
    }

