    probes_pressure
    {
        type            probes;
        libs               ("libsampling.so");
        enabled         true;
        timeStart       0.05;
        writeControl    runTime;
        writeInterval   5e-5;

        fields        (p pGauge);

        probeLocations
        (
            ( 0.0 0.0  0.5) // N
            ( 0.5 0.0  0.0) // E
            ( 0.0 0.0 -0.5) // S
            (-0.5 0.0  0.0) // W
        );

    }

