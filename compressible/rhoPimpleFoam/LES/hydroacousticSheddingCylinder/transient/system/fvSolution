    "(p|Phi)"
    {
        solver          PCG;
        preconditioner  FDIC;
        tolerance       1e-20;
        relTol          0.05;
        maxIter         600;
    }

    pFinal
    {
        $p;
        tolerance       1e-08;
        relTol          0.0;
    }

    "(U|k|omega|nuTilda)"
    {
//        solver          smoothSolver;
//        smoother        symGaussSeidel;
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-20;
        relTol          0.1;
    }

    "(U|k|omega|nuTilda)Final"
    {
        $U;
        tolerance       1e-20;
        relTol          0.1;
    }

    "(h|e|hFinal|eFinal)"
    {
        $U;
        tolerance       1.0e-020;
        relTol          0.1;
    }
}

potentialFlow
{
    nNonOrthogonalCorrectors 10;
}

