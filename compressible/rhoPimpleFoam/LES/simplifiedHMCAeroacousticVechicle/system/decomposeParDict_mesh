FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
numberOfSubdomains #eval {$nCPU};

method          scotch;

hierarchicalCoeffs
{
    n               ( 6 4 6 );
    delta           0.001;
    order           xyz;
}

distributed   no;
roots ();

