FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          CG;
        preconditioner  Cholesky;
        tolerance       1e-10;
        relTol          0.001;
        minIter         1000;
        maxIter         1000;
    }
    "(U|tau.*)"
    {
        solver          BiCGStab;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          0.001;
        minIter         2;
        maxIter         2;
    }
}

PISO
{
    nCorrectors         2;
    nNonOrthogonalCorrectors 0;
    pRefCell            0;
    pRefValue           0;
}

relaxationFactors
{
    U                   0.3;
    p                   0.3;
    tau                 0.3;
    tauM1               0.3;
    tauM2               0.3;
    tauM3               0.3;
    tauM4               0.3;
    tauM5               0.3;
    tauM6               0.3;
}

