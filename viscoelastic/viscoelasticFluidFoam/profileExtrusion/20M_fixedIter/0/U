FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0.002);
    }
    outlet
    {
        type            zeroGradient;
    }
    wall1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wall2
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wall3
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    symH
    {
        type            symmetryPlane;
    }
    symV
    {
        type            symmetryPlane;
    }
}

